<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.savestudents">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SaveStudents">
        <activity
            android:name="br.com.savestudents.view.activity.CreateTimelineActivity"
            android:exported="false" />
        <activity
            android:name="br.com.savestudents.view.activity.SelectOptionActivity"
            android:exported="false" />
         <activity
         android:name="br.com.savestudents.view.activity.CreateSubjectActivity"
         android:exported="false" />
        <activity
            android:name="br.com.savestudents.view.activity.TimelineActivity"
            android:exported="false" />
        <activity
            android:name="br.com.savestudents.view.activity.FilterOptionsActivity"
            android:exported="false" />
        <activity
            android:name="br.com.savestudents.view.activity.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>